    DefaultTableModel modelo;
    String[] titulos={"Codigo","Nombres","Apellidos","Fecha de Nac","Sexo","email","Telefono"};
    String[] datos=new String[7];
    

    /**
     * Creates new form insertar_usuario
     */
    public insertar_usuario() {
        initComponents();
       modelo=new DefaultTableModel(null,titulos);
       tblusuario.setModel(modelo);
    }
    void CargarTabla(Integer valor){
        String sSQL="";
        modelo=new DefaultTableModel(null,titulos);
        conexion mysql=new conexion();
        Connection cn=mysql.conectar();
        sSQL="SELECT * FROM usuario WHERE idusuario LIKE '% "+valor+"%'";
        try{
            Statement st=cn.createStatement();
            ResultSet rs=st.executeQuery(sSQL);
            while(rs.next()){
                datos[0]=rs.getString("idusuario");
                datos[1]=rs.getString("nombre");
                datos[2]=rs.getString("apellidos");
                datos[3]=rs.getString("fecha_nacimiento");
                datos[4]=rs.getString("sexo");
                datos[5]=rs.getString("e_mail");
                datos[6]=rs.getString("numero_tel");
                modelo.addRow(datos);
            }
            tblusuario.setModel(modelo);
        }catch(SQLException ex){
            JOptionPane.showMessageDialog(null, "Error "+ex, "Error", JOptionPane.ERROR_MESSAGE);
        }
    }

